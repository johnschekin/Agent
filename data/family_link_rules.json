      "family_id": "debt_capacity.indebtedness",
      "description": "Match indebtedness covenant sections in the negative covenants article. Covers 'Indebtedness', 'Limitation on Indebtedness', 'Incurrence of Indebtedness', and similar heading variants.",
      "version": 1,
      "status": "published",
      "owner": "bootstrap",
      "article_concepts": ["negative_covenants"],
      "filter_dsl": "heading: Indebtedness | \"Limitation on Indebtedness\" | \"Incurrence of Indebtedness\" | \"Limitation on Incurrence of Indebtedness\"",
      "heading_filter_ast": {
        "type": "group",
        "operator": "or",
        "children": [
          {"type": "match", "value": "Indebtedness"},
          {"type": "match", "value": "Limitation on Indebtedness"},
          {"type": "match", "value": "Incurrence of Indebtedness"},
          {"type": "match", "value": "Limitation on Incurrence of Indebtedness"}
        ]
      },
      "required_defined_terms": [
        "Permitted Indebtedness",
        "Indebtedness"
      ],
      "excluded_cue_phrases": null,
      "template_overrides": null
